{
    "name": "armincms/dorehami",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/passport" : "~0.1",
        "armincms/truth-or-dare" : "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Dorehami\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Dorehami\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
